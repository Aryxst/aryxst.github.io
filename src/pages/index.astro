---
import { SEO } from 'astro-seo';
import Layout from '@/layouts/WithMDX.astro';
import { Content } from '@/mdx/index.mdx';
import '@fontsource/poppins/600.css';
// 400+600

const title = 'Aryxst';
---

<SEO
 {title}
 canonical={Astro.url}
 description="Hi, I'm Aryxst!"
 openGraph={{
  basic: {
   title,
   type: 'website',
   url: Astro.url.origin,
   image: Astro.url.origin + '/aryxst.jpg',
  },
 }}
/>
<Layout {Content} />
